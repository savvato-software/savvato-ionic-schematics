{
  "$schema": "http://json-schema.org/schema",
  "$id": "page",
  "title": "page schema",
  "type": "object",
  "properties": {
    "name": {
      "type": "string",
      "description": "Domain Object Name",
      "x-prompt": "What is the domain object name?"
    },
    "path": {
      "type": "string",
      "description": "The path to create the page.",
      "default": "app/pages",
      "visible": false
  }},
  "required": ["name"]
}


